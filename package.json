{
  "name": "ng-if-responsive",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "lint-staged": "lint-staged --allow-empty",
    "commitlint": "commitlint --edit",
    "watch": "ng build --watch --configuration development",
    "test:library": "jest --config projects/ng-if-responsive/jest.config.js",
    "coverage:library": "jest --config projects/ng-if-responsive/jest.config.js --coverage",
    "format:test": "prettier --list-different 'projects/**/*.{ts,html}'",
    "format:write": "prettier --write 'projects/**/*.{ts,html}'",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@briebug/jest-schematic": "^5.0.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "29.2.4",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "jest": "28.1.3",
    "jest-preset-angular": "^12.2.3",
    "lint-staged": "^13.1.0",
    "ng-packagr": "^15.0.3",
    "ngx-highlightjs": "^7.0.1",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.8.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,less,md,json}": "prettier --write"
  }
}
